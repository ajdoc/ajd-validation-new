{
    "name": "ajd/ajd-validation-filter",
    "description": "Validation and Filtering Library",
    "license": "MIT",
    "authors": [
        {
            "name": "Aj Doc"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": ">=8.1",
        "ajd/ajd-dic": "dev-main",
        "ajd/ajd-expression": "dev-main"
    },
    "repositories": {
        "ajd/ajd-expression": {
            "type": "git",
            "url": "https://github.com/ajdoc/ajd-expression.git",
            "reference": "main",
            "options": {
                "symlink": true
            }
        }
    },
    "autoload": {
        "psr-4": {
            "AjdVal\\": "src"
        }
    },
    "require-dev": {
        "pestphp/pest": "2.x-dev",
        "marwanalsoltany/mighty": "^1.2@dev"
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
